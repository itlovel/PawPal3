<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PawPal3"
        tools:targetApi="31">
        <activity
            android:name=".mealplanner.base.BaseActivity" />
        <activity
            android:name=".mealplanner.activities.MakeYourOwnMenuActivity" />
        <activity
            android:name=".mealplanner.activities.CustomizedMenuActivity" />
        <activity
            android:name=".mealplanner.activities.CartActivity" />
        <activity
            android:name=".mealplanner.activities.FoodSelectionActivity" />
        <activity
            android:name=".mealplanner.activities.YourCustomMenuActivity" />
        <activity
            android:name=".mealplanner.activities.FoodRecommendationActivity" />
        <activity
            android:name=".mealplanner.activities.MealPlannerActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"/>
    </application>

</manifest>